{
	"keyword": "<%= params[:search_field] %>",
	"results": [
		<% phrases.each do |p| %>
			{
			"title": "<%= p.title %>",
			"slug": "<%= p.slug %>",
			"group_name": "<%= p[:group_name] %>",
			"group_slug": "<%= p[:group_slug] %>"
		} <% if p != phrases.last %>,<% end %>
		<% end %>
	],
	"wikipedia": [
		<% wikiphrases.each_with_index do |(title, desc), index| %>
			{
			"title": "<%= title %>",
			"href": "<%= show_phrase_from_wikipedia_path(title.gsub(' ', '_')) %>"
		}<% if index != wikiphrases.size-1 %>,<% end %>
		<% end %>
	]
}